        <Label x:Name="lbel_ErgebnisHZ2" Content="Label" HorizontalAlignment="Left" Margin="169,46,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Width="63"/>
        <Label x:Name="label_ErgebnisPenalty" Content="Label" HorizontalAlignment="Left" Margin="169,72,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Width="68" Height="23"/>
        <Label x:Name="label_Mannschaftsname2" Content="Label" Grid.Column="1" HorizontalAlignment="Left" Margin="56.4,46,0,0" VerticalAlignment="Top" Width="100"/>

